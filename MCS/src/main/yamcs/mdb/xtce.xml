            <Parameter name="ADCS_heater_setpoint" parameterTypeRef="int8_degC" shortDescription="ADCS Heater Setpoint (°C)"/>
            <Parameter name="ADCS_power_draw" parameterTypeRef="float_W" shortDescription="ADCS Power Draw (W)"/>
