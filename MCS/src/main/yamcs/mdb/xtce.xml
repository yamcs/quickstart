			<SequenceContainer name="TelemetryPacket">
				<EntryList />
				<BaseContainer containerRef="CCSDSPacket">
					<RestrictionCriteria>
						<ComparisonList>
							<Comparison value="0" parameterRef="CCSDS_Packet_ID/Version" />
							<Comparison value="0" parameterRef="CCSDS_Packet_ID/Type" />
						</ComparisonList>
					</RestrictionCriteria>
				</BaseContainer>
			</SequenceContainer>

