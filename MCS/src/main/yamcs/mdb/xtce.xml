            <Parameter name="ADCS_angular_rate_z" parameterTypeRef="float_deg_s" shortDescription="ADCS Angular Rate (z) (deg/s)"/>	
			<Parameter name="ADCS_angular_rate_total" parameterTypeRef="float_deg_s" shortDescription="ADCS Angular Rate Total (deg/s)"/>
