				<IntegerDataEncoding sizeInBits="8" />
			</IntegerParameterType>

			<!-- Power Balance Type -->
			<IntegerParameterType name="PowerBalance_Type" shortDescription="Power Balance (0=BALANCED, 1=POSITIVE, 2=NEGATIVE)">	
				<IntegerDataEncoding sizeInBits="8"/>	
			</IntegerParameterType>

