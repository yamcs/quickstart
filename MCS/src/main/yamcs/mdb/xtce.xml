					<Unit>A</Unit>
				</UnitSet>
				<FloatDataEncoding sizeInBits="32" />
			</FloatParameterType>
			<FloatParameterType name="float_percent">
				<UnitSet>
					<Unit>%</Unit>
				</UnitSet>
				<FloatDataEncoding sizeInBits="32" />
			</FloatParameterType>
			<FloatParameterType name="float_deg_s">
