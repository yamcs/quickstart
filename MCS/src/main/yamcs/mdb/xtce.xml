			<Parameter name="POWER_solar_panel_generation_pY" parameterTypeRef="float_W" shortDescription="POWER Solar Panel Generation (pY) (W)"/>
			<Parameter name="POWER_solar_panel_generation_nY" parameterTypeRef="float_W" shortDescription="POWER Solar Panel Generation (nY) (W)"/>
			
            <!-- ADCS Parameters -->
            <Parameter name="ADCS_state" parameterTypeRef="SubsystemState_Type" shortDescription="ADCS State (0=OFF, 1=IDLE, 2=ACTIVE, 3=ERROR)"/>
