            <Parameter name="COMMS_temperature" parameterTypeRef="int8_degC" shortDescription="COMMS Temperature (°C)"/>
            <Parameter name="COMMS_heater_setpoint" parameterTypeRef="int8_degC" shortDescription="COMMS Heater Setpoint (°C)"/>
            <Parameter name="COMMS_power_draw" parameterTypeRef="float_W" shortDescription="COMMS Power Draw (W)"/>
